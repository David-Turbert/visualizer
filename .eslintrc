{
  "env": {
    "browser": true,
    "amd": true
  },
  "rules": {
    "brace-style": [2, "1tbs", {"allowSingleLine": true}],
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "eol-last": 2,
    "indent": [2, 4],
    "new-parens": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-label": 2,
    "no-ex-assign": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-irregular-whitespace": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-with": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "space-after-keywords": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "strict": [2, "global"],
    "use-isnan": 2,
    "valid-typeof": 2
  }
}